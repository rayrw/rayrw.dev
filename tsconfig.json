{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", ".svg-ts/virtual-module.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"strictNullChecks": true,
		"baseUrl": ".",
		"paths": {
			"~/layouts/*": ["src/layouts/*"],
			"~/pages/*": ["src/pages/*"],
			"~/components/*": ["src/components/*"],
			"~/common/*": ["src/common/*"],
			"~/assets/*": ["src/assets/*"]
		}
	}
}
